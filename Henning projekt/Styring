void moveBall(*Tvector vector){
  vector->a=vector->a+vector->x;
  vector->b=vector->b+vector->y;
  }
  
void reflectBallSide(*Tvector vector){
  vector->x=~vector->x+1;
}

void reflectBallTop(*Tvector vector){
  vector->y=~vector->y+1;  
}

